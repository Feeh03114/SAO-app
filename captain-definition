{
  "schemaVersion": 2,
  "dockerfileLines": [
    "FROM node:18.16.0-alpine",
    "ENV PORT=80",
    "ENV NODE_ENV=production",
    "ENV NEXT_TELEMETRY_DISABLED=1",
    "EXPOSE 80",
    "WORKDIR /usr/src/app",
    "COPY package*.json ./",
    "RUN npm install --force && npm cache clean --force",
    "COPY . .",
    "RUN npm run build",
    "CMD [ \"npm\", \"start\" ]"
  ]
}

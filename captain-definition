{
  "schemaVersion": 2,
  "dockerfileLines": [
    "FROM node:18.16.0-alpine",
    "ENV PORT=80",
    "ENV NODE_ENV=production",
    "ENV NEXT_TELEMETRY_DISABLED=1",
    "EXPOSE 80",
    "WORKDIR /usr/src/app",
    "COPY package.json yarn.lock ./",
    "RUN yarn install && yarn cache clean --force",
    "COPY . .",
    "RUN yarn build",
    "CMD [ \"yarn\", \"start\" ]"
  ]
}
